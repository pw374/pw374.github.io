# {< let _ = print_endline title >}

<div class='blogpost'>
  <div class='blogpost-contents'>
    {< include Post(Unit) >}
  </div>
  <div class='blogpost-footer'>
    <span class='date'>{< let _ = print_endline date >}</span>
    {<
      let _ =
         List.iter (fun t -> printf "<a href='%s' class='tag'>%s</a>" t t) tags
    >}
  </div>
</div>

